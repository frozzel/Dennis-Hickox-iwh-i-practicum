doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1=`${title}`
        .center
            a(href='/update-cobj') Add to this table
        table.table 
            tr.table__header 
                th Name
                th Type
                th Age
            tbody
                each pet in data
                    tr
                        td #{pet.properties.name}  
                        td   #{pet.properties.type}
                        td  #{pet.properties.age}